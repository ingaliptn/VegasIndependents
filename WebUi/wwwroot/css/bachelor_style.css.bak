	padding:0;
}
.navbar{
	margin-bottom:0;
	border-radius:0;
}
.vegas_navbar{
	background:transparent !important;
	border:0 none;
	min-height:20px;
	border-radius:0;	
	/*border:0 none;*/
}

.navbar-nav > li{
	border-right:1px solid #633a08;
}

.navbar-inverse .navbar-nav > li > a {
	background:transperent !important;
	min-height:45px;
	font-family:'Droid Sans', Tahoma, Geneva, sans-serif;
	font-size:14px;
	font-weight:700;
	text-transform:uppercase;
	text-align:center;
	outline:0 none;		
	color:#2c2b2b;
	transition:all .2s ease 0s; -moz-transition:all .2s ease 0s; -webkit-transition:all .2s ease 0s; -o-transition:all .2s ease 0s;
	border-right:1px solid #efcf6e;
}
.navbar-inverse .navbar-nav > li > a:hover, .navbar-inverse .navbar-nav > li > a:focus, .navbar-inverse .navbar-nav > li > a.active{
	color:#FFF;
	outline:0 none;
	/*border-bottom:5px solid #002176;*/
	background:url(../images/navbar_bg_hover.jpg) repeat-x;
}
.nav > li > a{
	padding:10px 39px;
}
.mobile_btn{
	background:#442807;
}
