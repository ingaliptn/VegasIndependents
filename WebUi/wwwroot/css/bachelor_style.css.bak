/* navigation block */
.navigation_bar_bg{
	background:url(../images/navbar_bg.jpg) repeat-x left top;
	height:45px;
	padding:0 15px;
	border-bottom-left-radius:10px;
		-moz-border-bottom-left-radius:10px; -webkit-border-bottom-left-radius:10px; -o-border-bottom-left-radius:10px;
	border-bottom-right-radius:10px;
		-moz-border-bottom-right-radius:10px; -webkit-border-bottom-right-radius:10px; -o-border-bottom-right-radius:10px;
	box-shadow:0 2px 4px #4e4e4e;
		-moz-box-shadow:0 2px 4px #4e4e4e; -webkit-box-shadow:0 2px 4px #4e4e4e; -o-box-shadow:0 2px 4px #4e4e4e;
}

.main_navitgation{
	padding-right:0 !important;
	padding-top:0;
	padding:0;
}
