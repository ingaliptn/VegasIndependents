}
a {
  color: #0088cc;
  text-decoration:none;
  outline:0 none;
}

a:hover,
a:focus {
  color: #005580;
  text-decoration: none;
  outline:0 none;
}

p{
	font-family:'OpenSansRegular', Arial, Tahoma, Geneva, sans-serif;
	font-weight:400;
	text-align:justify;
	font-size:13px;
	line-height:18px;
